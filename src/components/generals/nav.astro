---
import type {
} from 'astro';

import { getCollection } from 'astro:content';
import type { CollectionEntry } from 'astro:content';

import {generalObject } from '@schemas/index';
import SidebarLinkItemHTMLAttributesSchema  from '@astrojs/starlight';

const DEBUG = true;

const generals = new Array<generalObject>();

const generalEntries: CollectionEntry<'generals'>[] | undefined = await getCollection('generals');

generalEntries.map((entry: CollectionEntry<'generals'>) => {
  const general: generalObject = entry.data;
  generals.push({
    'label': general.id,


  })
});
